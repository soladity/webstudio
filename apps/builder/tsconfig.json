{
  "extends": "../../packages/tsconfig/remix.json",
  "include": [
    "remix.env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "../../@types/**/*.d.ts"
  ],
  "compilerOptions": {
    "paths": {
      "~/*": ["app/*"]
    },
    "baseUrl": ".",
    "rootDir": "./app",
    "declaration": false,
    "emitDeclarationOnly": false,
    "declarationDir": null,
    "noEmit": true
  }
}
