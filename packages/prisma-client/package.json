{
  "name": "@webstudio-is/prisma-client",
  "version": "0.1.0",
  "description": "Webstudio Prisma layer",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./src --ext .ts,.tsx --max-warnings 0",
    "checks": "yarn typecheck && yarn lint",
    "dev": "prisma generate --watch && tsup --watch",
    "build": "prisma format && prisma generate && rm -fr lib tsconfig.tsbuildinfo && tsc"
  },
  "bin": {
    "migrations": "./migrations-cli/cli.ts"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.4",
    "prisma": "^4.3.1",
    "tsup": "^6.1.3",
    "typescript": "4.7.4"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@inquirer/confirm": "^0.0.25-alpha.0",
    "@prisma/client": "^4.3.1",
    "@types/minimist": "^1.2.2",
    "dotenv": "^16.0.0",
    "execa": "^6.1.0",
    "minimist": "^1.2.6",
    "tsx": "^3.9.0",
    "umzug": "^3.2.1",
    "uuid": "^9.0.0",
    "zod": "^3.19.0"
  },
  "sideEffects": false,
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": "cjs",
    "outDir": "lib"
  }
}
