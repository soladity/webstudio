{
  "name": "@webstudio-is/react-sdk",
  "version": "0.25.0",
  "description": "Webstudio JavaScript / TypeScript API",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "type": "module",
  "scripts": {
    "dev": "build-package --watch",
    "build": "build-package",
    "build:args": "generate-arg-types './src/components/*.tsx !./src/**/*.stories.tsx !./src/**/*.ws.tsx' && prettier --write \"**/*.props.json\"",
    "typecheck": "tsc --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint ./src --ext .ts,.tsx --max-warnings 0",
    "checks": "pnpm typecheck && pnpm lint && pnpm test",
    "storybook:run": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@esbuild-kit/esm-loader": "^2.4.2",
    "@jest/globals": "^29.3.1",
    "@remix-run/node": "1.12.0",
    "@remix-run/react": "1.12.0",
    "@remix-run/server-runtime": "1.12.0",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.11",
    "@types/node": "^18.11.18",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@webstudio-is/generate-arg-types": "workspace:^",
    "@webstudio-is/jest-config": "workspace:^",
    "@webstudio-is/scripts": "workspace:^",
    "@webstudio-is/storybook-config": "workspace:^",
    "@webstudio-is/tsconfig": "workspace:^",
    "babel-loader": "^8.2.5",
    "esbuild": "^0.14.25",
    "esbuild-node-externals": "^1.4.1",
    "jest": "^29.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix-utils": "^4.1.0",
    "tsx": "^3.9.0",
    "typescript": "4.7.4",
    "zod": "^3.19.1"
  },
  "peerDependencies": {
    "@remix-run/react": "1.9.0",
    "@remix-run/server-runtime": "1.9.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix-utils": "^4.1.0",
    "zod": "^3.19.1"
  },
  "dependencies": {
    "@nanostores/react": "^0.4.1",
    "@webstudio-is/asset-uploader": "workspace:^",
    "@webstudio-is/css-data": "workspace:^",
    "@webstudio-is/icons": "workspace:^",
    "@webstudio-is/image": "workspace:^",
    "@webstudio-is/prisma-client": "workspace:^",
    "@webstudio-is/project-build": "workspace:^",
    "detect-font": "^0.1.5",
    "mitt": "^3.0.0",
    "nanostores": "^0.7.1",
    "warn-once": "^0.1.1"
  },
  "exports": {
    "import": "./lib/index.js",
    "require": "./lib/cjs/index.cjs"
  },
  "types": "src/index.ts",
  "files": [
    "lib/*",
    "src/*",
    "!*.test.*"
  ],
  "license": "MIT",
  "private": false,
  "sideEffects": false
}
