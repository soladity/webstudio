{
  "name": "@webstudio-is/css-vars",
  "version": "0.17.0",
  "description": "CSS vars utility",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
