{
  "name": "@webstudio-is/project",
  "version": "0.17.0",
  "description": "Webstudio Project",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "checks": "yarn typecheck && yarn lint && yarn test",
    "dev": "build-package --watch",
    "build": "build-package",
    "lint": "eslint ./src --ext .ts,.tsx --max-warnings 0",
    "publish-to-npm": "bash ../../bin/publish-to-npm.sh"
  },
  "dependencies": {
    "@webstudio-is/design-tokens": "*",
    "@webstudio-is/fonts": "*",
    "@webstudio-is/css-engine": "*",
    "@webstudio-is/asset-uploader": "*",
    "@webstudio-is/css-data": "*",
    "@webstudio-is/prisma-client": "*",
    "@webstudio-is/react-sdk": "*",
    "bson-objectid": "^2.0.2",
    "immer": "^9.0.12",
    "nanoid": "^3.2.0",
    "react": "^17.0.2",
    "slugify": "^1.6.5",
    "uuid": "^9.0.0",
    "warn-once": "^0.1.1",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@webstudio-is/scripts": "*",
    "@webstudio-is/tsconfig": "*",
    "typescript": "4.7.4"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/cjs/index.cjs"
    },
    "./server": "./server.js"
  },
  "types": "src/index.ts",
  "files": [
    "lib/*",
    "src/*",
    "!*.test.*"
  ],
  "license": "MIT",
  "private": false,
  "sideEffects": false
}
