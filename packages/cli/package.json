{
  "name": "@webstudio-is/cli",
  "version": "0.91.0",
  "description": "Webstudio CLI",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "type": "module",
  "bin": {
    "webstudio": "./lib/bin.js"
  },
  "files": [
    "lib/*",
    "src/*",
    "templates/*",
    "bin.js",
    "!*.test.*"
  ],
  "scripts": {
    "typecheck": "tsc",
    "checks": "pnpm typecheck",
    "build:templates": "tsx ./templates/build.ts",
    "build": "rm -rf ./lib && esbuild ./src/**/*.ts --outdir=./lib",
    "local-run": "tsx --no-warnings ./src/bin.ts"
  },
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@webstudio-is/http-client": "workspace:^",
    "@webstudio-is/image": "workspace:^",
    "@webstudio-is/react-sdk": "workspace:^",
    "@webstudio-is/sdk": "workspace:^",
    "@webstudio-is/sdk-components-react": "workspace:^",
    "@webstudio-is/sdk-components-react-radix": "workspace:^",
    "@webstudio-is/sdk-components-react-remix": "workspace:^",
    "env-paths": "^3.0.0",
    "ora": "^7.0.1",
    "p-limit": "^4.0.0",
    "strip-indent": "^4.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@webstudio-is/form-handlers": "workspace:^",
    "@webstudio-is/scripts": "workspace:^",
    "@webstudio-is/tsconfig": "workspace:^",
    "esbuild": "^0.18.18",
    "tsx": "^3.12.6",
    "typescript": "5.2.2"
  }
}
