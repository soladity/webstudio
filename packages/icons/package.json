{
  "name": "@webstudio-is/icons",
  "version": "0.1.1",
  "description": "Webstudio Icons",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "scripts": {
    "dev": "tsup --watch",
    "build": "rm -fr lib tsconfig.tsbuildinfo && tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./src --ext .ts,.tsx --max-warnings 0",
    "checks": "yarn typecheck && yarn lint"
  },
  "devDependencies": {
    "tsup": "^6.1.3",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.1.0",
    "@webstudio-is/css-vars": "*"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*",
    "README.md",
    "!*.test.*"
  ],
  "license": "MIT",
  "private": false,
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": "cjs",
    "outDir": "lib"
  }
}
