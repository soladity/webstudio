{
  "name": "@webstudio-is/react-sdk",
  "version": "0.7.0",
  "description": "Webstudio JavaScript / TypeScript API",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "scripts": {
    "dev": "tsup --watch",
    "build": "yarn build:lib",
    "build:args": "tsx ./bin/generate-arg-types.ts './src/components/*.tsx !./src/**/*.stories.tsx !./src/**/*.ws.tsx'",
    "typecheck": "tsc --noEmit",
    "build:lib": "rm -fr lib tsconfig.tsbuildinfo && tsc",
    "test": "jest",
    "lint": "eslint ./src --ext .ts,.tsx --max-warnings 0",
    "checks": "yarn typecheck && yarn lint && yarn test",
    "prepublishOnly": "yarn checks && yarn build:lib",
    "storybook:run": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@esbuild-kit/esm-loader": "^2.4.2",
    "@remix-run/react": "^1.2.3",
    "@remix-run/server-runtime": "^1.2.3",
    "remix-utils": "^4.1.0",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-webpack4": "^6.5.6",
    "@storybook/manager-webpack4": "^6.5.6",
    "@storybook/react": "^6.5.6",
    "@storybook/testing-library": "^0.0.11",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.21",
    "@webstudio-is/jest-config": "*",
    "@webstudio-is/prisma-client": "*",
    "babel-loader": "^8.2.5",
    "esbuild": "^0.14.25",
    "esbuild-node-externals": "^1.4.1",
    "react": "^17.0.2",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^17.0.2",
    "tsup": "^6.1.3",
    "tsx": "^3.9.0",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "@remix-run/react": "^1.2.3",
    "@remix-run/server-runtime": "^1.2.3",
    "remix-utils": "^4.1.0",
    "@storybook/react": "^6.5.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@webstudio-is/asset-uploader": "^*",
    "@webstudio-is/fonts": "*",
    "@webstudio-is/icons": "*",
    "@webstudio-is/css-data": "*",
    "@webstudio-is/jest-config": "*",
    "immer": "^9.0.12",
    "mitt": "^3.0.0",
    "react-nano-state": "^0.4.0",
    "warn-once": "^0.1.1",
    "zod": "^3.17.3"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*",
    "README.md",
    "!*.test.*"
  ],
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": "cjs",
    "outDir": "lib"
  }
}
