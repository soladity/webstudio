{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["lib/**", "dist/**", "build/**", "api/**", "public/build/**"]
    },
    "checks": {
      "dependsOn": ["^checks"],
      "outputs": []
    },
    "storybook:build": {
      "dependsOn": ["@webstudio-is/icons#build"],
      "cache": false
    }
  }
}
