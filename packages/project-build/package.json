{
  "name": "@webstudio-is/project-build",
  "version": "0.86.0",
  "description": "Webstudio Project Build",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./lib/types/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./index.server": {
      "source": "./src/index.server.ts",
      "types": "./lib/types/index.server.d.ts",
      "import": "./lib/index.server.js"
    }
  },
  "files": [
    "lib/*",
    "src/*",
    "!*.test.*"
  ],
  "sideEffects": false,
  "scripts": {
    "typecheck": "tsc",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "checks": "pnpm typecheck && pnpm test",
    "dev": "build-package --watch",
    "build": "build-package",
    "dts": "tsc --project tsconfig.dts.json"
  },
  "dependencies": {
    "@webstudio-is/css-data": "workspace:^",
    "@webstudio-is/prisma-client": "workspace:^",
    "@webstudio-is/trpc-interface": "workspace:^",
    "nanoid": "^4.0.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@webstudio-is/jest-config": "workspace:^",
    "@webstudio-is/scripts": "workspace:^",
    "@webstudio-is/tsconfig": "workspace:^",
    "jest": "^29.6.2",
    "type-fest": "^4.2.0",
    "typescript": "5.1.6"
  }
}
