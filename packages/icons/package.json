{
  "name": "@webstudio-is/icons",
  "version": "0.1.1",
  "description": "Webstudio Icons",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "scripts": {
    "dev": "tsup --watch",
    "build": "rm -fr lib tsconfig.tsbuildinfo && tsc",
    "generate": "rm -fr src/gen && tsx ./node_modules/.bin/svgo-jsx svgo-jsx.config.ts && tsx ./bin/svgo-jsx-index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./src --ext .ts,.tsx --max-warnings 0",
    "checks": "yarn typecheck && yarn lint",
    "storybook:run": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-webpack4": "^6.5.6",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack4": "^6.5.6",
    "@storybook/react": "^6.5.6",
    "@storybook/testing-library": "^0.0.13",
    "@svgo/jsx": "^0.3.1",
    "tsup": "^6.1.3",
    "tsx": "^3.9.0",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.1.0",
    "@webstudio-is/css-vars": "*"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*",
    "README.md",
    "!*.test.*"
  ],
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": "cjs",
    "outDir": "lib"
  }
}
