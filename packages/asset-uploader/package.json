    "dev": "concurrently \"tsc -w\" \"tsc-alias -w\"",
    "checks": "yarn typecheck && yarn test",
    "prepublishOnly": "yarn checks && yarn build"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.131.0",
    "@aws-sdk/lib-storage": "^3.131.0",
    "@babel/core": "^7.17.12",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "concurrently": "^7.2.2",
    "esbuild-jest": "^0.5.0",
    "jest": "^27.5.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.6.2",
    "zod": "^3.17.3"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "README.md",
    "!*.test.*"
  ],
  "license": "MIT",
  "private": false,
  "dependencies": {
    "tsc-alias": "^1.7.0"
  }
