{
  "name": "@webstudio-is/project",
  "version": "0.51.0",
  "description": "Webstudio Project",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "checks": "pnpm typecheck && pnpm lint",
    "dev": "build-package --watch",
    "build": "build-package",
    "dts": "tsc --emitDeclarationOnly --declaration --declarationDir lib/types",
    "lint": "eslint ./src --ext .ts,.tsx --max-warnings 0"
  },
  "dependencies": {
    "@trpc/server": "^10.9.0",
    "@webstudio-is/asset-uploader": "workspace:^",
    "@webstudio-is/prisma-client": "workspace:^",
    "@webstudio-is/project-build": "workspace:^",
    "@webstudio-is/trpc-interface": "workspace:^",
    "nanoid": "^3.2.0",
    "slugify": "^1.6.5",
    "uuid": "^9.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.4",
    "@webstudio-is/scripts": "workspace:^",
    "@webstudio-is/tsconfig": "workspace:^",
    "typescript": "5.0.3"
  },
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./lib/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./server": {
      "source": "./src/index.server.ts",
      "default": "./server.js"
    }
  },
  "types": "lib/types/index.d.ts",
  "files": [
    "lib/*",
    "src/*"
  ],
  "license": "MIT",
  "private": false,
  "sideEffects": false
}
